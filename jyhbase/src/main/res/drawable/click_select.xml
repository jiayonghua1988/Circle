<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--点击选中状态定义 -->
    <!-- 未选中 -->
    <item android:drawable="@android:color/white" android:state_pressed="false"/>
    <!-- 选中 -->
    <item android:drawable="@color/color_EFEFEF" android:state_pressed="true"/>
    <item android:drawable="@color/color_EFEFEF" android:state_focused="true"></item>

</selector>